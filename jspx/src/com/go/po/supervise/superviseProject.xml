<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="superviseProject">
	
	<!-- 添加后台数据 -->
	<insert id="add" parameterType="java.util.HashMap" >
		<foreach collection="list" item="vo">
	   		insert into supervise_project(id,superviseId,projectId,status)
	  		 values(#{vo.id},#{vo.superviseId},#{vo.projectId},'启用');
		</foreach>
	</insert>
	<!-- 删除数据 -->
	<delete id="delete"  parameterType = "java.util.List">
        delete from   supervise_project   Where 1 = 1
	    And superviseId in 
	    <foreach collection="list" item="id"  open="(" separator="," close=")">   
           #{id}  
        </foreach> ;
	</delete>
</mapper>
